<!doctype html>
<!--[if IE 9]><html class="lt-ie10" lang="en" > <![endif]-->
<!--[if IE 10]><html class="ie10" lang="en" > <![endif]-->
<html class="no-js" lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>
        <%=@getPreparedTitle()%>
    </title>
    <meta name="description" content="<%= @getPreparedDescription() %>" />
    <meta name="keywords" content="<%= @getPreparedKeywords() %>" />
    <%-@getBlock( 'meta').toHTML()%>
    <%-@getBlock( 'styles').add(@site.styles).toHTML()%>
            <script src="/js/modernizr.js"></script>
</head>

<body>

    <header id="header">
        <div class="row">
            <div class="large-12 columns left">
                <div id="logo-image">
                    <a href="<%=@site.url%>">
                        <%-@site.title%>
                    </a>
                </div>

            </div>

        </div>
    </header>

    <div class="">
        <!-- Header and Nav -->
        <nav class="top-bar">
            <div class="row">
                <ul class="title-area">
                    <!-- Title Area -->
                    <li class="toggle-topbar menu-icon">
                        <a href="#">
                            <span>Menu</span>
                        </a>
                    </li>
                </ul>

                <section class="top-bar-section">
                    <!-- Left Nav Section -->
                    <ul class="left">
                        <li class=" menu-icon active" id="home-icon">
                            <a href="/">
                                <span class="name">Home</span>
                            </a>
                        </li>
                        <li class=" menu-icon" id="blog-icon">
                            <a href="/blog/">
                                <span class="name">Blog</span>
                            </a>

                        </li>
                        <li class=" menu-icon" id="contact-icon">
                            <a href="/#contact">
                                <span class="name">Contact</span>
                            </a>
                        </li>
                    </ul>

                    <ul class="title-area" id="menu-titles">
                        <!-- Title Area -->
                        <li class="name hidden" id="home-text">
                            <h1>
                            <a href="/">Home
                            </a>
                        </h1>
                        </li>
                        <li class="name hidden" id="blog-text">
                            <h1>
                            <a href="/blog/">Blog
                            </a>
                        </h1>
                        </li>
                        <li class="name hidden" id="contact-text">
                            <h1>
                            <a href="/#contact">Contact
                            </a>
                        </h1>
                        </li>

                    </ul>

                    <ul class="right">
                        <li class=" menu-icon">
                            <a href="#">
                                <i class="icon-facebook"></i>
                            </a>
                        </li>
                        <li class=" menu-icon">
                            <a href="#">
                                <i class="icon-twitter"></i>
                            </a>

                        </li>
                        <li class=" menu-icon">
                            <a href="atom.xml">
                                <i class="icon-feed"></i>
                            </a>
                        </li>
                    </ul>
                </section>
            </div>
        </nav>
    </div>
    <div class="content-wrapper">
        <section id="main-content">
          <%-@content%>
        </section>
    </div>


    <div class="page-footer-bottom">
        <div class="row">
            <div class="medium-4 medium-4 push-8 columns">

                <ul class="home-social">
                    <li>
                        <a href="http://www.twitter.com/" class="twitter"></a>
                    </li>
                    <li>
                        <a href="http://www.facebook.com/" class="facebook"></a>
                    </li>
                    <li>
                        <a href="/contact" class="mail"></a>
                    </li>
                </ul>
            </div>
            <div class="medium-8 medium-8 pull-4 columns">
                <ul class="site-links">
                    <li class="name">
                        <a href="/"><%-@site.title%></a>
                    </li>
                    <li>
                        <a href="/">Home</a>
                    </li>
                    <li>
                        <a href="/blog">Blog</a>
                    </li>
                    <li>
                        <a href="/news">News & Events</a>
                    </li>
                </ul>
                <p class="copyright">Â©  <%-@thisYear%> <%-@site.title%>. All rights reserved.</p>
            </div>
        </div>
         <%-@getBlock( 'scripts').add(@site.scripts,{defer:false}).toHTML()%>
</body>

</html>
